<Application x:Class="EventManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EventManager"
             StartupUri="LoanStand.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Order is important -->

                <ResourceDictionary Source="pack://application:,,,/SourceWeave.Controls;component/Themes/SWStyles.xaml"/>
                <ResourceDictionary Source="Style.xaml"/>
                <ResourceDictionary Source="Colors.xaml"/>
                <ResourceDictionary Source="Fonts.xaml" />
                <ResourceDictionary Source="Button.xaml"/>
                <ResourceDictionary Source="Text.xaml" />
                <ResourceDictionary Source="GroupBox.xaml"/>
                <ResourceDictionary Source="Window.xaml"/>
                
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
